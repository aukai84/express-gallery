
<div id="sign-in">

    <div id="gallery-container">

        {{#each photos}}
        <div class="photo-card">
            <h3>{{author}} - {{description}}</h3>
            <a href="/gallery/{{{id}}}">
                <img src="{{link}}" class="image-box">
             </a>
            <h3>{{shortLink}} Post By - {{user.username}}</h3>
        </div>
        {{/each}}
    </div>
</div>
