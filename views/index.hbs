<header>
    <div id="top-logo">_ARCHITEKT</div>
</header>
<div id="photo-container"></div>
{{#each photos}}
<div class="photo-card">
    <div class="photo">
    <a href="/gallery/{{{id}}}">
        <img src="{{link}}">
        </a>
    </div>
    <h2>author: {{author}}</h2>
    <h3>link: {{link}}</h2>
    <h4>description: {{description}}</h2>
</div>
{{/each}}
</div>